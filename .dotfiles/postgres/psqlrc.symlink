-- set prompt
\set PROMPT1 '%[%033[1;33m%]%M:%>%[%033[0m%] %[%033[1;36m%]%n@%/%[%033[0m%]\n%R%#%x '
\set PROMPT2 '%R%# '

-- set string to print for NULL content
\pset null '[null]'

-- complete in upper casoe
\set COMP_KEYWORD_CASE upper

-- display query times
\timing

-- history settings
\set HISTSIZE 2000
\set HISTCONTROL ignoredups

-- beautify large table output
\x auto

